{
  "name": "system-interface",
  "version": "1.0.0",
  "description": "The interface between CPET systems and the service.",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "lint:fix": "npm run lint -- --fix 'src/**/*.{js,ts,jsx,tsx}'",
    "prettier": "prettier --check 'src/**/*.{js,ts,jsx,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/EnhancedCPET/system-interface.git"
  },
  "author": "Apurva Shah shahh.apurva@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EnhancedCPET/system-interface/issues"
  },
  "homepage": "https://github.com/EnhancedCPET/system-interface#readme",
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
